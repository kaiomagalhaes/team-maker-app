{
  "name": "team-maker",
  "version": "0.0.1",
  "description": "Team Maker App, for managing teams!",
  "main": "index.js",
  "scripts": {
    "dev": "next dev ./src",
    "build": "next build ./src",
    "start": "next start ./src",
    "test": "NODE_ENV=test jest --watch",
    "test-pre-push": "NODE_ENV=test jest",
    "lint": "$(npm bin)/stylelint './src/**/*.js' && $(npm bin)/eslint /src"
  },
  "author": "scinocco.a@gmail.com",
  "license": "MIT",
  "dependencies": {
    "apollo-boost": "^0.1.16",
    "apollo-client": "^2.4.2",
    "apollo-link": "^1.2.6",
    "apollo-link-rest": "^0.7.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-styled-components": "^1.7.1",
    "dotenv": "^6.2.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "graphql": "^14.0.2",
    "graphql-anywhere": "^4.1.24",
    "graphql-tag": "^2.9.2",
    "next": "^7.0.0",
    "next-with-apollo": "^3.1.3",
    "nprogress": "^0.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-adopt": "^0.6.0",
    "react-apollo": "^2.2.1",
    "react-dom": "^16.5.2",
    "styled-components": "^3.4.9"
  },
  "devDependencies": {
    "@babel/code-frame": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "casual": "^1.5.19",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-react-app": "^1.0.5",
    "eslint-import-resolver-alias": "^1.1.1",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.34.0",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "7.1.0",
    "graphql-tools": "^4.0.0",
    "jest": "^23.6.0",
    "jest-resolve": "^23.6.0",
    "jest-transform-graphql": "^2.1.0",
    "pre-push": "^0.1.1",
    "stylelint": "^9.9.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^2.0.0",
    "stylelint-processor-styled-components": "^1.5.1"
  },
  "pre-push": [
    "test-pre-push",
    "lint"
  ],
  "jest": {
    "setupTestFrameworkScriptFile": "./jest.setup.js",
    "modulePaths": [
      "src"
    ],
    "moduleNameMapper": {
      "^components$": "<rootDir>/src/components",
      "^connected-components$": "<rootDir>/src/connected-components",
      "^lib$": "<rootDir>/src/lib",
      "^pages$": "<rootDir>/src/pages",
      "^theme$": "<rootDir>/src/theme",
      "^config$": "<rootDir>/config"
    }
  }
}
